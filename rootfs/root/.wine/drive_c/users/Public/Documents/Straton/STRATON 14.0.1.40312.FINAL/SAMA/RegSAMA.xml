<?xml version="1.0" encoding="utf-8"?>
<!--
		TAG BLOCK:
				BLOCK			: the block name (type)
				NAME			: the long name of the block (for display)
				DESC			: description of the block
			  ICON      : the text to be displayed in control
				HELP			: the help page
				CX				: the default block width
				CY				: the default block height
				MAX_CX		: for extensible blocks, the maximum width
				MIN_CX		: for resizable blocks, the minimum with
	  		MAX_CY		: for extensible blocks, the maximum height
				MIN_CY		: for resizable blocks, the minimum height
	  		DX				: for resizable blocks, the step for resizing width
				DY				: for resizable blocks, the step for resizing height
				CUSTOMDRAW: indicates that block drawing is not standard
											0: standard drawing
											1: full height is used, and width is standard
											2: centered rectangle plus vertical line on left of block
											3: centered ellipse plus vertical line on left of block
											4: diamond
											5: full rectangle drawing
											6: centered ellipse
											7: trapez
											8: trapez + rect on top
											9: emf file drawing
											10: the drawing comes from symbol editor -> block can contain a list of another blocks
											11: OR horizontal
											12: AND horizontal
				CATEGORY  : block category (for sorting)
											Arithmetic
											PID
											Non linear
											Comparison
											Limiting
											Memory
											Selector
										  Boolean
											Time
											Signal
											Symbol
			  PREVIEW	 : indicates if HMi can display preview box for this block
				USERSYMBOL: indicates that symbol has been added by user interface
				DRAWINGFILE: the file to be drawn in this block (can be null)
		TAG PIN:
				NAME			: the pin name (must be unic in one block)
				DIRECTION : direction of pin
											IN       : input pin
											OUT      : output pin
											INTERNAL : internal parameter
											PARAM    : parameter
				DATATYPE  : pin data type
											REAL
											BOOL
											TIME
											DINT
				DESC			: pin description
				DEFVALUE	: default value
				POSITION  : side where pin is connected
											LEFT
											RIGHT
											TOP
											BOTTOM
				INDEX			: index where pin is connected (begin at 0 for the first pin)
											-1: indicates that pin is centered on side
				CANDRAWPINRECT: indictaes if pin rectangle can be displayed on block
				CANDRAWPINLABEL: indictaes if pin label can be displayed aroud block
				INSIDEX		: used to draw inside pins (if not empty, complete connection go into block)
				INSIDEY   : same as INSIDEX
				
		TAG CUSTOMDRAW: list of graphic objects
				TAG GRAPHIC OBJECT (RECT / ELLIPSE / DIAMOND / TRAPEZ / EMF / AND / OR / LINE / ARROW / ARROW2)
						ID: id of the graphical object
						X: left of graphic object (float)
						Y: top of graphic object (float)
						CX: width of graphic object (float)
						CY: height of graphic object (float)
						FONT: text font
						FONTSIZE: text font size
						FONTUNDERLINE: text is underlined
						FONTBOLD: text font is bold
						FONTITALIC: text font is italic
						RGBTEXT: text color
						RGBBACK: background color
						RGBLINE: line color
						LINEWIDTH: line width
						EMFFILE: emf file (for graphical objects 'EMF' only)
						ICON: the icon to display in the middle of the object
									icon can take one of this values: SUM / AVERAGE / DIFF / DIV / ROOT / EXPT / HIGHSELECT / LOWSELECT / HIGHLIMIT / LOWLIMIT / HILOWLIMIT / VELLIMIT / BIAS / Ro / So
-->
